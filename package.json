{
  "name": "formango",
  "type": "module",
  "version": "3.2.0-beta.4",
  "description": "",
  "author": "Wouter Laermans <wouter.laermans@appwise.be>",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "form",
    "form validation",
    "validation",
    "zod",
    "schema",
    "formango"
  ],
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "pub:beta": "pnpm publish --no-git-checks --access public --tag beta",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vue": "^3.2.47",
    "zod": "^3.24.1"
  },
  "dependencies": {
    "@vue/devtools-api": "^6.6.4",
    "clone-deep": "^4.0.1"

  },
  "devDependencies": {
    "@antfu/ni": "^24.3.0",
    "@antfu/utils": "^9.2.0",
    "@standard-schema/spec": "1.0.0",
    "@types/clone-deep": "^4.0.4",
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@wisemen/eslint-config-vue": "1.2.0-beta.3",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.5.1",
    "pnpm": "^10.8.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.12.1",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vite": "^6.2.6",
    "vitepress": "1.6.3",
    "vitest": "^3.1.1",
    "vue": "^3.5.13"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
